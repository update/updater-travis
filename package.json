{
  "name": "updater-travis",
  "description": "Update .travis.yml based on preferences.",
  "version": "0.2.0",
  "homepage": "https://github.com/update/updater-travis",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "update/updater-travis",
  "bugs": {
    "url": "https://github.com/update/updater-travis/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "templates",
    "updatefile.js",
    "utils.js"
  ],
  "main": "index.js",
  "preferGlobal": true,
  "engines": {
    "node": ">=4.7"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "delete": "^0.3.2",
    "fs-exists-sync": "^0.1.0",
    "generate-travis": "^0.3.1",
    "get-value": "^2.0.6",
    "is-valid-app": "^0.2.1",
    "js-yaml": "^3.8.1",
    "lazy-cache": "^2.0.2",
    "semver": "^5.3.0",
    "set-value": "^0.4.3",
    "through2": "^2.0.3",
    "union-value": "^1.0.0",
    "unset-value": "^1.0.0"
  },
  "devDependencies": {
    "copy": "^0.3.0",
    "gulp-format-md": "^0.1.11",
    "mocha": "^3.2.0",
    "npm-install-global": "^0.1.2",
    "update": "^0.7.4"
  },
  "keywords": [
    "fix",
    "lint",
    "linter",
    "maintain",
    "maintainance",
    "plugin",
    "plugins",
    "travis",
    "up-to-date",
    "update",
    "update-plugin",
    "update-updater",
    "updateplugin",
    "updater",
    "updaterplugin",
    "upkeep"
  ],
  "update": {
    "run": "global"
  },
  "verb": {
    "toc": {
      "method": "preWrite"
    },
    "layout": "updater",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "updater-editorconfig",
        "updater-eslint",
        "updater-license"
      ]
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "update"
    ]
  }
}
